CREATE DATABASE IF NOT EXISTS microservice;

CREATE TABLE `microservice`.`distribuido` (
  `id` INT auto_increment,
  `descripcion` VARCHAR(150) NULL,
  `rss` VARCHAR(500) NULL,
  `revista` VARCHAR(100) NULL,
  `opcional` VARCHAR(10) NULL,
  `token_str` VARCHAR(50) NULL,
  `paginas` VARCHAR(100) NULL,
  `token_num` VARCHAR(50) NULL,
  PRIMARY KEY (`id`));

select * from microservice.distribuido;

insert into microservice.distribuido ( descripcion, rss, revista, opcional, token_str, paginas, token_num) VALUES ('Chinas trade deficit and structural worries in Europe, among the main issues', 'http://www.fxstreet.com/analysis/strategic-currency-briefing/2014/03/10/', 'FXstreet.com', 'b', 'dPhGU51DcrolUIMxbRm0InaHGA2XM', 'www.fxstreet.com', '1394470503887' );

 select count(*) from microservice.distribuido;
